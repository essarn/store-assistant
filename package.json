{
  "name": "store-assistant",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "lint": "eslint --ext .ts --fix .",
    "format": "prettier --write **/*.ts"
  },
  "turbo": {
    "baseBranch": "origin/main",
    "npmClient": "pnpm",
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ]
      },
      "dev": {
        "cache": false
      }
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "turbo": "^1.0.24",
    "typescript": "^4.5.4"
  }
}
